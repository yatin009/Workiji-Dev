        // PRELOADER
        #preloader
            .clock
                .arrow_sec
                .arrow_min
        #wrap
            // HEADER BEGIN
            header#header
                .container
                    a.logo(href='#')
                        img(src='./images/logo_f.png', alt='Best start for your business', height='80', width='85')
                    a.login_btn(href='#') Login
                    ul.soc_nav
                        li
                            a.icon-soc-googleplus(href='#')
                        li
                            a.icon-soc-facebook(href='#')
                        li
                            a.icon-soc-twitter(href='#')
            // HEADER END
            // INTRO BEGIN
            section#intro
                .container
                    .row
                        .col-lg-6.col-md-7.col-sm-6
                            .slogan
                                h2.wow.fadeInRight(data-wow-delay='0.6s', data-wow-duration='0.5s') Best start
                                h3.wow.fadeInLeft(data-wow-delay='0.9s', data-wow-duration='0.5s') for your business
                                p.wow.fadeInRight(data-wow-delay='1.2s')
                                    | The Workiji mobile app manages on-demand service requests for buildings including Hotels,
                                    | Apartments, Condominium, and other type of buildings.
                        .col-lg-4.col-lg-offset-2.col-md-5.col-sm-6
                            form#register_form(role='form')
                                .title.wow.flipInX(data-wow-duration='0.6s')  Register now
                                .form-group
                                    input#name.form-control.wow.flipInX(type='text', data-wow-delay='0.8s', data-wow-duration='0.2s', placeholder='Full name', name='name')
                                .form-group
                                    input#email.form-control.wow.flipInX(type='email', data-wow-delay='1s', data-wow-duration='0.2s', placeholder='Email Address', name='email')
                                .form-group
                                    input#username.form-control.wow.flipInX(type='text', data-wow-delay='1.2s', data-wow-duration='0.2s', placeholder='Address', name='username')
                                .form-group
                                    input#password.form-control.wow.flipInX(type='password', data-wow-delay='1.4s', data-wow-duration='0.2s', placeholder='Contact Number', name='password')
                                button.btn.btn_start.wow.flipInX(type='submit', data-wow-delay='1.6s', data-wow-duration='0.2s') Start
                #slides(data-stellar-ratio='0.4')
                    .slides-container
                        img(src='images/1.jpg', alt='')
                        img(src='images/2.jpg', alt='')
                        img(src='images/3.jpg', alt='')
            // INTRO END
            // SERVICES BEGIN
            section#services
                .container
                    .row
                        .col-md-4.service
                            i.icon-drawer
                            h4 Service Request(SR) Inbox Queue
                            ul(type='disc')
                                li(type='disc') Use Mobile Phone to snap image of maintenance issues &amp; SMS to a central phone number
                                li(type='disc') Deliver SMS SR to Central Inbox
                                li(type='disc') Building Agent Central Inbox
                                li(type='disc') Pull to Refresh Notification for SR that is being worked upon
                                li(type='disc') Filter SR status

                        .col-md-4.service
                            i.icon-badge
                            h4 Triage, Dispatch, and Approval for SR
                            ul(type='disc')
                                li(type='disc') Intelligent SR parsing &amp; dispatch to Contractor
                                li(type='disc') Receive Quotation from Contractors
                                li(type='disc') Approval &amp; eSignature for Quotation
                        .col-md-4.service
                            i.icon-map
                            h4 Map View for all SR and Work Orders
                            ul(type='disc')
                                li(type='disc') Geocoding of SR &amp; Work Orders
                                li(type='disc') Navigate PINs on map for all SR &amp; Work Orders
                        .clearfix.visible-lg.visible-md
                        .col-md-4.service
                            i.icon-users
                            h4 Contractor Management
                            ul(type='disc')
                                li(type='disc') Manage all SR assigned to Contractors
                                li(type='disc') Keep track of Work Orders
                                li(type='disc') Track progress of Contractors $spend
                        .col-md-4.service
                            i.icon-wrench
                            h4 Work Management
                            ul(type='disc')
                                li(type='disc') Generate &amp; Schedule a Work Order
                                li(type='disc') Timeline view to track Progress of Work Orders
                                li(type='disc') Timeline view to track Progress of Work Orders
                        .col-md-4.service
                            i.icon-emoticon-smile
                            h4 User Management
                            ul(type='disc')
                                li(type='disc') Authenticate Users with Firebase Authentication
                                li(type='disc') Manage all Users for an Organization and for Buildings
                                li(type='disc') Manage Contractors assigned to Organization
                        .clearfix.visible-lg.visible-md
                        .col-md-4.service
                            i.icon-home
                            h4 Building Management
                            ul(type='disc')
                                li(type='disc') Setup Buildings per Organization
                                li(type='disc') Create or Import Building data (floors &amp; rooms)
                                li(type='disc') Track maintenance Service Requests for all Suites
                        .col-md-4.service
                            i.icon-graph
                            h4 Real Time Analytics Dashboard
                            ul(type='disc')
                                li(type='disc') Analytics Summary for all SR and Work Orders
                                li(type='disc') Set up Custom Events for Dashboard Monitoring
                                li(type='disc') Switch between pie, line graph views
                        .col-md-4.service
                            i.icon-calendar
                            h4 Calendar Appointment Scheduler
                            ul(type='disc')
                                li(type='disc') Voice Command to create Calendar Event for Work Order
                                li(type='disc') Navigate Work Orders from Calendar
